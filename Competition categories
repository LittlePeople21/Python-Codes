idade = int(input('Quantos ano se tem?\nR: '))

if idade <= 9:
    
    print('Tu vai ficar na categoria Mirim')
    
elif idade > 9 and idade <= 14:
    
    print('Tu vai ficar na categoria Infantil')
    
elif idade > 14 and idade <= 19:
    
    print('Tu vai ficar na categoria Junior')
    
elif idade == 20:
    
    print('Tu vai ficar na categoria SÃªnior')
    
else:
    
    print('Tu vai ficar na categoria master')
